{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learing express",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "cross-env NODE_ENV=development ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
